class Solution {
public:
    int maxArea(vector<int>& height) {
     int i=0;
        int n=height.size();
        int j=n-1;
        int mx=INT_MIN;
        while(i<j){
            int volume = min(height[i],height[j])*(j-i);
            mx=max(mx,volume);
            if(height[i]>height[j])j--;
            else i++;
        }
        return mx;
    }
};
